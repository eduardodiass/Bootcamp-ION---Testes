@model IEnumerable<Tarefas.web2.Models.TarefaModel>

<table class="table table-dark">
    <thead>
        <th scope="col">
            @Html.DisplayNameFor(model => model.Id)
            
        </th>
        <th scope="col">
            @Html.DisplayNameFor(model => model.Titulo)

        </th>

        <th scope="col">
            @Html.DisplayNameFor(model => model.Descricao)
            
        </th>
        th scope="col">
            @Html.DisplayNameFor(model => model.DataInicio)
            
        </th>
        th scope="col">
            @Html.DisplayNameFor(model => model.DataFim)
            
        </th>
        <th scope="col">
           <p>Ação</p>
            
        </th>
    </thead>
    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Titulo
                </td>
                <td>
                    @item.Descricao

                 </td>
                 <td>
                    @item.DataInicio

                 </td>
                 <td>
                    @item.DataFim

                 </td>
                 <td>
                     <a asp-asp-controller="Tarefas" asp-action="Details" asp-route-id="@item.Id">Detalhes</a>
                 </td>

        </tr>

        </tbody>
         
    }
   