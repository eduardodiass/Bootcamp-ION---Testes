@model Tarefas.web2.Models.TarefaModel;


<form asp-action="Create">
  <div class="form-group">
    @Html.LabelFor(model => model.Id, new{@class="form-label"})
    @Html.TextBoxFor(model => model.Id,new{@class="form-control",@readonly = "readonly"} )
  </div>

  <div class="form-group">
    @Html.LabelFor(model => model.Titulo, new{@class="form-label"})
    @Html.TextBoxFor(model => model.Titulo,new{@class="form-control",@readonly = "readonly"} )
  </div>

  <div class="form-group">
    @Html.LabelFor(model => model.Descricao, new{@class="form-label"})
    @Html.TextBoxFor(model => model.Descricao,new{@class="form-control",@readonly = "readonly"} )
  </div>

  <div class="form-group">
    @Html.LabelFor(model => model.DataInicio, new{@class="form-label"})
    @Html.TextBoxFor(model => model.DataInicio,  new{@class="form-control",@readonly = "readonly"})
  </div>

  <div class="form-group">
    @Html.LabelFor(model => model.DataFim, new{@class="form-label"})
    @Html.TextBoxFor(model => model.DataFim,new{@class="form-control",@readonly = "readonly"} )
  </div>
</form>
<br>
<a asp-asp-controller="Tarefa" asp-action="Index">Voltar</a>